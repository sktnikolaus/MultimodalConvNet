name: "LeNet"
layer {
  name: "data"
  type: "HDF5Data"
  top: "X"
  top: "label_landscape"
  top: "label_wildlife"
  top: "label_travel"
  top: "label_vacation"
  top: "label_sunrise"
  top: "label_sunset"
  top: "label_night"
  top: "label_art"
  top: "label_architecture"
  top: "label_urban"
  top: "label_abandoned"
  top: "label_beautiful"
  top: "label_cute"
  top: "label_love"
  top: "label_beauty"
  top: "label_summer"
  top: "label_autumn"
  top: "label_winter"
  include {
    phase: TRAIN
  }
  hdf5_data_param {
    source: "train_h5_list.txt"
    batch_size: 12
  }
}
layer {
  name: "data"
  type: "HDF5Data"
  top: "X"
  top: "label_landscape"
  top: "label_wildlife"
  top: "label_travel"
  top: "label_vacation"
  top: "label_sunrise"
  top: "label_sunset"
  top: "label_night"
  top: "label_art"
  top: "label_architecture"
  top: "label_urban"
  top: "label_abandoned"
  top: "label_beautiful"
  top: "label_cute"
  top: "label_love"
  top: "label_beauty"
  top: "label_summer"
  top: "label_autumn"
  top: "label_winter"
  include {
    phase: TEST
  }
  hdf5_data_param {
    source: "data/val_h5_list.txt"
    batch_size: 24
  }
}
layer {
  bottom: "X"
  top: "conv1_1"
  name: "conv1_1"
  type: "Convolution"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size:3
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  bottom: "conv1_1"
  top: "conv1_1"
  name: "relu1_1"
  type: "ReLU"
}
layer {
  bottom: "conv1_1"
  top: "conv1_2"
  name: "conv1_2"
  type: "Convolution"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  }
  blobs_lr: 0
  blobs_lr: 0
}
layer {
  bottom: "conv1_2"
  top: "conv1_2"
  name: "relu1_2"
  type: "ReLU"
}
layer {
  bottom: "conv1_2"
  top: "pool1"
  name: "pool1"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}
layer {
