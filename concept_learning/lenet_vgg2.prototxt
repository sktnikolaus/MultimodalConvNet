  }
  blobs_lr: 0
  blobs_lr: 0
}
layer {
  name: "loss"
  type: "SoftmaxLoss"
  bottom: "fc8"
  bottom: "label"
  top: "loss/loss"
}
layer {
  name: "accuracy/top1"
  type: "Accuracy"
  bottom: "fc8"
  bottom: "label"
  top: "accuracy@1"
