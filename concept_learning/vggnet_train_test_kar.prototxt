  top: "label_cute"
  top: "label_love"
  top: "label_beauty"
  top: "label_summer"
  top: "label_autumn"
  top: "label_winter"
  top: "label_spring"
  include {
    phase: TRAIN
  }
  hdf5_data_param {
    source: "data/train_h5_list.txt"
    batch_size: 24
  }
}
layer{
  name: "data"
